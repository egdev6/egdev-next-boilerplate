{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "autoprefixer": "10.0.1",
    "eslint": "8",
    "eslint-config-next": "14.1.4",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8",
    "prettier": "3.2.5",
    "tailwindcss": "3.3.0"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
